use('business');
//db.restaurants.find()

// exe 1
db.restaurants.find({
  rating: {
    $not: {
      $lte: 5
    }
  }
}).count() 

// exe 2
db.restaurants.find({
  $or: [
    { rating: { $lte: 6 } },
    { borough: "Brooklyn" }
  ]
}).count();

// exe 3
db.restaurants.find({
  $and: [
    { 
      borough:{
        $in:['Queens' , 'Staten Island', 'Broklyn']
      }
    },
    {
      rating: { $gte: 4}
    }
  ]
}).count();

